{
  "name": "paulprae-com",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered career platform â€” paulprae.com",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "ingest": "tsx scripts/ingest-linkedin.ts",
    "generate": "tsx scripts/generate-resume.ts",
    "export": "tsx scripts/export-resume.ts",
    "export:pdf": "tsx scripts/export-resume.ts --format pdf",
    "export:docx": "tsx scripts/export-resume.ts --format docx",
    "pipeline": "npm run ingest && npm run generate && npm run export && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:pipeline": "vitest run tests/pipeline.test.ts",
    "test:unit": "vitest run tests/ingest.test.ts tests/generate.test.ts tests/export.test.ts tests/config.test.ts",
    "lint": "eslint --cache --cache-location .next/cache/eslint/ .",
    "lint:fix": "eslint --cache --cache-location .next/cache/eslint/ --fix .",
    "format": "prettier --write \"**/*.{ts,tsx,js,mjs,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,mjs,json,css,md}\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "dotenv": "^17.3.1",
    "next": "^16.1.6",
    "papaparse": "^5.5.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.3.1",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/eslint-plugin": "^1.6.9",
    "eslint": "^9.39.3",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
